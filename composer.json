{
  "name": "rap2hpoutre/indice",
  "description": "better errors for beginner, intermediate, drunk or memory-less people",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "rap2h",
      "email": "raphaelht@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "stripe",
    "billing"
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/contracts": "^5.5",
    "illuminate/support": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "laravel/framework": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "Rap2hpoutre\\Indice\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rap2hpoutre\\Indice\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rap2hpoutre\\Indice\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
